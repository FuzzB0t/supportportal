<div class="col-md-12">
  <div class="well middle-content">
    <h1>Licenses</h1>

    <table class="CRUD-table">
      <thead>
        <tr>
          <th>Title</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @licenses.each do |license| %>
          <tr id="license_index_tr">
            <td><%= license.title %></td>
            <div class="CRUD-table-actions">
              <td><%= link_to 'Show', license %> | </td>
              <td><%= link_to 'Edit', edit_license_path(license) %> | </td>
              <td><%= link_to 'Destroy', license, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </div>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'Import New License', new_license_path %>
  </div>
</div>